        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientDesktop"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="215.993">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="52*"/>
            <ColumnDefinition Width="53*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="DeleteUser" Grid.ColumnSpan="2" Content="Delete" HorizontalAlignment="Left" Margin="66,292,0,0" VerticalAlignment="Top" Width="75" Click="DeleteUser_Click"/>
        <TextBox x:Name="UserId" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="23" Margin="43,182,0,0" TextWrapping="Wrap" Text="UserId" VerticalAlignment="Top" Width="120"/>

    </Grid>
